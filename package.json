{
  "name": "@blackglory/wait-for",
  "version": "0.7.2",
  "description": "A module for waiting for things to happen.",
  "keywords": [
    "waitFor",
    "DOM"
  ],
  "files": [
    "lib"
  ],
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "repository": "git@github.com:BlackGlory/wait-for.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "vitest --run",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "extra-abort": "^0.3.2",
    "husky": "^4.3.6",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "return-style": "^3.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-patch": "^2.1.0",
    "tslib": "^2.5.0",
    "typescript": "4.8",
    "typescript-transform-paths": "^3.4.6",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.3",
    "yarn-deduplicate": "^6.0.1"
  },
  "dependencies": {
    "@blackglory/observe": "^0.2.0",
    "@blackglory/structures": "^0.13.1",
    "extra-promise": "^6.0.4",
    "extra-timers": "^0.2.5",
    "iterable-operator": "^4.0.3",
    "rxjs": "^7.8.0"
  }
}
